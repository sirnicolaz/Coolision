<?xml version="1.0"?>
<config>
	<modules>
        <Coolision_Layer_Filter_Attribute>
            <version>0.1.0</version>
        </Coolision_Layer_Filter_Attribute>
        <Coolision_Layer_Filter_Item>
            <version>0.1.0</version>
        </Coolision_Layer_Filter_Item>
	<Coolision_Layer_Filter_Price>
	  <version>0.1.0</version>
	</Coolision_Layer_Filter_Price>
    </modules>
    <global>
        <models>
            <catalog>
                <rewrite>
                    <layer_filter_attribute>Coolision_Catalog_Model_Layer_Filter_Attribute</layer_filter_attribute>
                </rewrite>
            </catalog>
			<catalog>
                <rewrite>
                    <layer_filter_item>Coolision_Catalog_Model_Layer_Filter_Item</layer_filter_item>
                </rewrite>
            </catalog>
        </models>
	<blocks>
	  <catalog>
	    <rewrite>
              <layer_filter_price>Coolision_Catalog_Block_Layer_Filter_Price</layer_filter_price>
            </rewrite>
	  </catalog>
	</blocks>
    </global>
</config>
